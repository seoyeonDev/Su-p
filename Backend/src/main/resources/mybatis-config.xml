<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="org.postgresql.Driver"/>
                <property name="url" value="jdbc:postgresql://localhost:5432/studyproject"/>
                <property name="username" value="studyproject"/>
                <property name="password" value="studyproject"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="com/example/studyproject/mapper/memberMapper.xml"/>
    </mappers>
</configuration>
